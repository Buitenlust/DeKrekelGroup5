@model DeKrekelGroup5.ViewModel.MainViewModel
@Styles.Render("~/Content/uploadfile.css")
@Scripts.Render("~/bundles/jquery")
<div id="content">
    <h4 id="subtitel">
        Studenten importeren
        <div class="floatRight">
            <a href=' @Url.Action("index") '><img src="~/Images/Return.png" /></a>
        </div>
    </h4>
    <hr /><div id="wrapDetails">
        <div class="center">

            <div id="multiplefileuploader"></div>
            <a href="/Uitleners/SaveGroupUsers" class="button green" id="save" style="display: none; margin-top: 10px;" ><img src="~/Images/tick.png">Lijst Opslaan!</a>
            <div id="status">
            </div>
            <div id='uploadfiles'></div>

        </div>
              <div id="importlist">
                  @Html.Partial("ImportUitlenersLijst", Model)
              </div>
</div>
</div>
        @section JavaScript
        {
        <script src="/Scripts/uploadData.js" type="text/javascript"></script>
        <script src="/Scripts/jquery.validate.min.js" type="text/javascript"></script>
        <script src="/Scripts/jquery.validate.unobtrusive.js" type="text/javascript"></script>
        <script src="/Scripts/jQuery.MultiFile.min.js" type="text/javascript"></script>
        }
